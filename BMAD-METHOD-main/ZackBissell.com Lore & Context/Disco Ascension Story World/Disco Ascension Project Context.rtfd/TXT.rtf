import \{ useEffect, useState \} from "react";\
import \{ motion \} from "framer-motion";\
\
const redactedText = [\
  "Following the Mochakk @ Plaza de Espa\'f1a performance on April 17, 2023...",\
  "[REDACTED] Terry Hunter\'92s remix of \'93Workin\'92 Hard\'94 [REDACTED]",\
  "Plaza de Espa\'f1a registered a 73% spike in energy flow [REDACTED]",\
  "[REDACTED] dual-groove displacement effect confirmed [REDACTED]",\
  "ETAB tribunal scheduled to review causality chain and assign liability",\
];\
\
export default function RedactedDocument() \{\
  const [visibleLines, setVisibleLines] = useState(0);\
\
  useEffect(() => \{\
    const onScroll = () => \{\
      const maxLines = redactedText.length;\
      const trigger = window.scrollY / window.innerHeight;\
      const revealed = Math.floor(trigger * maxLines);\
      setVisibleLines(Math.min(revealed, maxLines));\
    \};\
    window.addEventListener("scroll", onScroll);\
    return () => window.removeEventListener("scroll", onScroll);\
  \}, []);\
\
  return (\
    <div className="bg-neutral-900 py-20 px-6 font-mono tracking-tight max-w-4xl mx-auto">\
      <h2 className="text-xl text-green-400 mb-4">\uc0\u55357 \u56770  ETAB REDACTED MEMO: SC-94.TEMP.TILT</h2>\
      <div className="space-y-4">\
        \{redactedText.map((line, i) => (\
          <motion.div\
            key=\{i\}\
            initial=\{\{ opacity: 0 \}\}\
            animate=\{\{ opacity: i < visibleLines ? 1 : 0 \}\}\
            transition=\{\{ duration: 0.4, delay: i * 0.1 \}\}\
            className="bg-black text-lime-200 px-4 py-2 border border-green-800 shadow-inner"\
          >\
            \{line\}\
          </motion.div>\
        ))\}\
      </div>\
    </div>\
  );\
\}\
